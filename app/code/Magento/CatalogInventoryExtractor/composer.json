{
    "name": "magento/module-catalog-inventory-extractor",
    "description": "Module extracts Catalog Inventory data which consumed by Magento_CatalogStorefrontConnector module",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~7.1.3||~7.2.0||~7.3.0",
        "magento/framework": "*",
        "magento/module-catalog-inventory": "*",
        "magento/module-configurable-product-extractor": "*",
        "magento/module-category-extractor": "*",
        "magento/module-catalog-storefront-connector": "*",
        "magento/module-store": "*"

    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Magento\\CatalogInventoryExtractor\\": ""
        }
    }
}
