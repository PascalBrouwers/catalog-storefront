        "sort-packages": true
    },
    "require": {
        "php": "~7.3.0||~7.4.0",
        "magento/framework": "*",
        "magento/module-catalog": "*",
        "magento/module-catalog-extractor": "*",
        "magento/module-configurable-product": "*",
        "magento/module-store": "*",
        "magento/module-eav": "*"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Magento\\ConfigurableProductExtractor\\": ""
        }
    }
