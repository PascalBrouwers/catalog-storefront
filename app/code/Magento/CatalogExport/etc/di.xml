    <preference for="Magento\CatalogExport\Model\Data\MetaInterface" type="Magento\CatalogExport\Model\Data\Meta" />
    <preference for="Magento\CatalogExport\Model\Data\ChangedAttributesInterface" type="Magento\CatalogExport\Model\Data\ChangedAttributes" />
    <preference for="Magento\CatalogExport\Api\Data\EntitiesRequestInterface" type="Magento\CatalogExport\Api\Data\EntitiesRequest" />
    <preference for="Magento\CatalogExport\Api\Data\EntityRequestDataInterface" type="Magento\CatalogExport\Api\Data\EntityRequestData" />

    <type name="Magento\CatalogExport\Model\ProductRepository">
